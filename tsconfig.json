{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "paths": {
    "react": [ "./node_modules/@types/react" ],
    "@app/*": ["./app/*"],
    "@assets/*": ["./assets/*"],
    "@components/*": ["./components/*"],
    "@styles/*": ["./styles/*"],
    "@utils/*": ["./utils/*"]
    }
}
